{
	"name": "@ultraq/array-utils",
	"version": "1.1.0",
	"description": "A collection of utilities for JavaScript arrays",
	"author": "Emanuel Rabina <emanuelrabina@gmail.com> (http://www.ultraq.net.nz/)",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ultraq/array-utils.git"
	},
	"keywords": [
		"array",
		"utilities",
		"flatten",
		"range",
		"remove"
	],
	"main": "array-utils.node.js",
	"module": "array-utils.js",
	"scripts": {
		"lint": "eslint array-utils.js test",
		"test": "npm run lint --silent && jest",
		"coverage": "cat ./coverage/lcov.info | coveralls",
		"build": "babel array-utils.js --out-file array-utils.node.js",
		"prepublish": "npm run build"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
		"coveralls": "^2.13.1",
		"eslint": "^4.8.0",
		"eslint-config-ultraq": "^2.0.0",
		"jest": "^21.2.1"
	}
}
